{
	"version": "0.2.0",
	"configurations": [
		{
			"name": "Run Extension (production)",
			"type": "extensionHost",
			"request": "launch",
			"args": [
				"--extensionDevelopmentPath=${workspaceFolder}",
				"--disable-workspace-trust",
				"C:\\Users\\samsung\\Desktop"
			],
			"outFiles": [
				"C:\\Users\\samsung\\Desktop\\dist\\**\\*.js"
			],
			"preLaunchTask": "${defaultBuildTask}",
			"env": {
				"IS_DEV": "true",
				"DEV_WORKSPACE_FOLDER": "C:\\Users\\samsung\\Desktop",
				"CLINE_ENVIRONMENT": "production",
				"USE_MODULAR_SYSTEM_PROMPT": "true"
			}
		},
		{
			"name": "Run Extension (staging)",
			"type": "extensionHost",
			"request": "launch",
			"args": [
				"--extensionDevelopmentPath=${workspaceFolder}",
				"--disable-workspace-trust",
				"C:\\Users\\samsung\\Desktop"
			],
			"outFiles": [
				"C:\\Users\\samsung\\Desktop\\dist\\**\\*.js"
			],
			"preLaunchTask": "${defaultBuildTask}",
			"env": {
				"IS_DEV": "true",
				"DEV_WORKSPACE_FOLDER": "C:\\Users\\samsung\\Desktop",
				"CLINE_ENVIRONMENT": "staging",
				"USE_MODULAR_SYSTEM_PROMPT": "true"
			}
		},
		{
			"name": "Run Extension (local)",
			"type": "extensionHost",
			"request": "launch",
			"args": [
				"--extensionDevelopmentPath=${workspaceFolder}",
				"--disable-workspace-trust",
				"C:\\Users\\samsung\\Desktop"
			],
			"outFiles": [
				"C:\\Users\\samsung\\Desktop\\dist\\**\\*.js"
			],
			"preLaunchTask": "${defaultBuildTask}",
			"env": {
				"IS_DEV": "true",
				"DEV_WORKSPACE_FOLDER": "C:\\Users\\samsung\\Desktop",
				"CLINE_ENVIRONMENT": "local",
				"USE_MODULAR_SYSTEM_PROMPT": "true"
			}
		},
		{
			"name": "Run Extension (Fresh Install Mode)",
			"type": "extensionHost",
			"request": "launch",
			"runtimeExecutable": "${execPath}",
			"args": [
				"--user-data-dir=C:\\Users\\samsung\\Desktop\\dist\\tmp\\user",
				"--profile-temp",
				"--sync=off",
				"--disable-extensions",
				"--extensionDevelopmentPath=${workspaceFolder}",
				"C:\\Users\\samsung\\Desktop"
			],
			"outFiles": [
				"C:\\Users\\samsung\\Desktop\\dist\\**\\*.js"
			],
			"preLaunchTask": "clean-tmp-user",
			"internalConsoleOptions": "openOnSessionStart",
			"postDebugTask": "stop",
			"env": {
				"IS_DEV": "true",
				"TEMP_PROFILE": "true",
				"DEV_WORKSPACE_FOLDER": "C:\\Users\\samsung\\Desktop",
				"CLINE_ENVIRONMENT": "production",
				"USE_MODULAR_SYSTEM_PROMPT": "true"
			}
		},
		{
			"type": "node",
			"request": "launch",
			"name": "Run cline-core service",
			"skipFiles": [
				"<node_internals>/**"
			],
			"sourceMaps": true,
			"resolveSourceMapLocations": [
				"${workspaceFolder}/**",
				"!**/node_modules/**"
			],
			"cwd": "C:\\Users\\samsung\\Desktop\\dist-standalone",
			"outFiles": [
